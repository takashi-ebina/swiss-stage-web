# Backend Environment Variables (.env.example)
# Copy this file to .env and fill in your actual values
# DO NOT commit .env to version control!

# Google OAuth2 Configuration (Development)
GOOGLE_CLIENT_ID=your-google-client-id-here.apps.googleusercontent.com
GOOGLE_CLIENT_SECRET=your-google-client-secret-here
GOOGLE_REDIRECT_URI=http://localhost:8080/login/oauth2/code/google

# JWT Configuration
JWT_SECRET_KEY=your-256-bit-secret-key-for-jwt-signature-here-please-use-strong-random-value
JWT_EXPIRATION_HOURS=24

# DynamoDB Configuration
AWS_REGION=ap-northeast-1
DYNAMODB_TABLE_NAME=swiss_stage_table
# For local development with DynamoDB Local
DYNAMODB_ENDPOINT=http://localhost:8000

# Application Configuration
SERVER_PORT=8080
FRONTEND_URL=http://localhost:3000

# Logging Level
LOGGING_LEVEL_ROOT=INFO
LOGGING_LEVEL_COM_SWISS_STAGE=DEBUG
